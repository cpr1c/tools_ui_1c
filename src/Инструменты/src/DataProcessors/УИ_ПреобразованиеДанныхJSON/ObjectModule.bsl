	СвойствоОбъектаИзСтруктуры(Объект,
							   СтруктураОбъекта,
							   "Дата",
							   ,
							   СписокСвойств,
							   ИсключаяСвойства);
	
	// TODO: Тут еще нужно проверить использование нумератора
	Если МетаОбъект.ДлинаНомера > 0 Тогда
		СвойствоОбъектаИзСтруктуры(Объект,
								   СтруктураОбъекта,
								   "Номер",
								   ,
								   СписокСвойств,
								   ИсключаяСвойства);
	КонецЕсли;
	
	Для Каждого ТекРеквизит Из МетаОбъект.Реквизиты Цикл
		СвойствоОбъектаИзСтруктуры(Объект,
								   СтруктураОбъекта,
								   ТекРеквизит.Имя,
								   ПолучатьБитыеСсылки,
								   СписокСвойств,
